{
  "name": "jison-board",
  "private": "true",
  "version": "0.0.1",
  "description": "React Leaderboard backed by ShareDB",
  "main": "server.js",
  "scripts": {
    "build": "mkdir -p dist/ && ./node_modules/.bin/browserify -t [ babelify --presets [ react ] ] client/index.jsx -o dist/bundle.js",
    "build:windows": "node_modules\\.bin\\browserify -t [ babelify --presets [ react es2015 ] ] client\\index.jsx -o dist\\bundle.js",
    "start": "node server/index.js"
  },
  "author": "Tom Coleman <tom@percolatestudio.com> (https://tom.thesnail.org/)",
  "contributors": [
    "Tom Coleman <tom@percolatestudio.com> (https://tom.thesnail.org/)",
    "Avital Oliver <avital@aoliver.org> (https://aoliver.org/)"
  ],
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "classnames": "^2.2.5",
    "connect": "^3.4.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "serve-static": "^1.11.1",
    "share-codemirror": "^0.1.0",
    "sharedb": "^1.0.0-beta",
    "sharedb-codemirror": "^0.2.1",
    "sharedb-mingo-memory": "^1.0.0-beta",
    "sharedb-mongo": "^1.0.0-beta.4",
    "sharedb-string-binding": "^1.0.0",
    "text-diff-binding": "^1.1.0",
    "through2": "^2.0.1",
    "underscore": "^1.8.3",
    "websocket-json-stream": "^0.0.3",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1"
  }
}
